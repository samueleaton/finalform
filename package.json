{
  "name": "finalform",
  "version": "1.2.1",
  "description": "HTML form fields parser",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint -c .eslintrc.json --max-warnings 10 src/**/*.js",
    "pretranspile": "npm run -s lint",
    "transpile": "babel src -d dist",
    "bundle": "webpack",
    "build": "npm run -s lint && npm run transpile && npm run bundle",
    "test": "http-server ./ -p 7654"
  },
  "keywords": [
    "html",
    "form",
    "forms",
    "fields",
    "parse",
    "html5"
  ],
  "author": "Sam Eaton",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "eslint": "^2.1.0",
    "http-server": "^0.9.0",
    "webpack": "^1.12.13"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samueleaton/finalform"
  },
  "dependencies": {
    "sparallel": "^1.0.0"
  }
}
